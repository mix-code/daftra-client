{
    "name": "mix-code/daftra-client",
    "description": "Simple implementation for Daftra",
    "keywords": [
        "mix-code",
        "mixcode",
        "daftra-client"
    ],
    "homepage": "https://github.com/mix-code/daftra-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Alaa El-Din",
            "email": "moaalaa16@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Hamza Omar Mohamed",
            "email": "hamzalilomar92@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MixCode\\DaftraClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MixCode\\DaftraClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MixCode\\DaftraClient\\DaftraClientServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "orchestra/testbench": "^9.12",
        "pestphp/pest": "^3.7"
    }
}
